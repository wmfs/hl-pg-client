{
  "name": "@wmfs/hl-pg-client",
  "version": "0.0.0-semantically-released",
  "description": "Provides a slightly higher level PostgreSQL client, that builds on pg-pool",
  "author": "West Midlands Fire Service",
  "homepage": "https://github.com/wmfs/hl-pg-client#readme",
  "keywords": [
    "tymly",
    "package",
    "postgresql"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmfs/hl-pg-client.git"
  },
  "bugs": {
    "url": "https://github.com/wmfs/hl-pg-client/issues"
  },
  "main": "./lib/index.js",
  "dependencies": {
    "debug": "4.3.4",
    "pg": "8.8.0"
  },
  "devDependencies": {
    "chai": "4.5.0",
    "codecov": "3.8.3",
    "conventional-changelog-metahub": "4.0.1",
    "cz-conventional-changelog": "3.3.0",
    "mocha": "10.7.3",
    "nyc": "17.1.0",
    "semantic-release": "24.1.2",
    "standard": "17.1.2",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1"
  },
  "scripts": {
    "lint": "standard",
    "test": "nyc mocha",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
