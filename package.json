{
  "name": "@wmfs/hl-pg-client",
  "version": "0.0.0-semantically-released",
  "description": "Provides a slightly higher level PostgreSQL client, that builds on pg-pool",
  "author": "West Midlands Fire Service",
  "homepage": "https://github.com/wmfs/hl-pg-client#readme",
  "keywords": [
    "tymly",
    "package",
    "postgresql"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmfs/hl-pg-client.git"
  },
  "bugs": {
    "url": "https://github.com/wmfs/hl-pg-client/issues"
  },
  "main": "./lib/index.js",
  "dependencies": {
    "debug": "4.0.0",
    "pg": "7.4.3"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "codecov": "3.1.0",
    "conventional-changelog-metahub": "2.0.2",
    "cz-conventional-changelog": "2.1.0",
    "mocha": "5.2.0",
    "nyc": "13.0.1",
    "semantic-release": "15.9.15",
    "standard": "12.0.1",
    "@semantic-release/changelog": "3.0.0",
    "@semantic-release/git": "7.0.4"
  },
  "scripts": {
    "lint": "standard",
    "test": "nyc mocha && standard",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
